{
    "name": "@parspec/pixel",
    "version": "1.0.0",
    "description": "Parspec UI component library",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "copy-files:esm": "copyfiles -u 1 src/**/*.css dist/esm",
        "copy-files:cjs": "copyfiles -u 1 src/**/*.css dist/cjs",
        "build": "rm -rf dist/ && npm run build:esm && npm run build:cjs && npm run copy-files:esm && npm run copy-files:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "author": "Abhijit Mitra",
    "license": "ISC",
    "dependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/icons-material": "^5.11.0",
        "@mui/material": "5.11.2",
        "@storybook/addons": "^6.5.16",
        "@storybook/theming": "^6.5.16",
        "@syncfusion/ej2-base": "~20.2.43",
        "@syncfusion/ej2-navigations": "~20.2.43",
        "@syncfusion/ej2-react-dropdowns": "^20.2.43",
        "@syncfusion/ej2-react-treegrid": "~20.2.43",
        "axios": "^1.3.3",
        "react-dropzone": "^14.2.3"
    },
    "devDependencies": {
        "@babel/core": "^7.20.7",
        "@storybook/addon-actions": "^6.5.15",
        "@storybook/addon-essentials": "^6.5.15",
        "@storybook/addon-interactions": "^6.5.15",
        "@storybook/addon-links": "^6.5.15",
        "@storybook/builder-webpack4": "^6.5.15",
        "@storybook/manager-webpack4": "^6.5.15",
        "@storybook/react": "^6.5.15",
        "@storybook/testing-library": "^0.0.13",
        "@types/node": "^18.11.17",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "babel-loader": "^8.3.0",
        "copyfiles": "^2.4.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^4.9.4"
    },
    "peerDependencies": {
        "axios": "^1.3.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
